package client;

import java.lang.reflect.Constructor;

import tv.*;

public class Main {

	public static void main(String[] args) {
		try {
			Constructor<?> c = Class.forName("tv.MockTvFactory").getDeclaredConstructor();
			c.setAccessible(true);
			Client client = new Client((TvFactory) c.newInstance());
			client.watchTv();
		} catch (Throwable e) {
			throw new RuntimeException(e);
		}
	}

}
